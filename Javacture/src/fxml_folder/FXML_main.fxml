<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" maxHeight="500.0" maxWidth="700.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainControl">
   <children>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
                  <Menu mnemonicParsing="false" text="Exporter...">
                    <items>
                      <MenuItem mnemonicParsing="false" onAction="#click_export_bmp" text="bmp" />
                        <MenuItem mnemonicParsing="false" onAction="#click_export_jpeg" text="jpeg" />
                        <MenuItem mnemonicParsing="false" onAction="#click_export_png" text="png" />
                    </items>
                  </Menu>
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
                  <Menu mnemonicParsing="false" text="Choix Agencement">
                    <items>
                      <MenuItem mnemonicParsing="false" onAction="#click_agencement_un" text="Agencement 1" />
                        <MenuItem mnemonicParsing="false" onAction="#click_agencement_deux" text="Agencement 2" />
                        <MenuItem mnemonicParsing="false" onAction="#click_agencement_trois" text="Agencement 3" />
                        <MenuItem mnemonicParsing="false" onAction="#click_agencement_quatre" text="Agencement 4" />
                    </items>
                  </Menu>
                  <MenuItem mnemonicParsing="false" onAction="#click_change_color" text="Changer Couleurs" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <VBox fx:id="_VBoxCentral" alignment="TOP_CENTER" />
   </children>
</VBox>
